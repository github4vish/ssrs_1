  <DataSets>
    <DataSet Name="Categories">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>
Select distinct(Name) Category from [Production].[ProductCategory]</CommandText>
      </Query>
      <Fields>
        <Field Name="Category">
          <DataField>Category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Subcategory">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@category">
            <Value>=Parameters!category.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
Select PSC.Name AS subcategory  from [Production].[ProductCategory] PC

Inner join [Production].[ProductSubcategory] PSC

On pc.ProductCategoryID=PSC.ProductCategoryID Where PC.Name in (@category)</CommandText>
      </Query>
      <Fields>
        <Field Name="subcategory">
          <DataField>subcategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@category">
            <Value>=Parameters!category.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@subcategory">
            <Value>=Parameters!subcategory.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>


Select P.Name AS Product  from [Production].[ProductCategory] PC

Inner join [Production].[ProductSubcategory] PSC

On pc.ProductCategoryID=PSC.ProductCategoryID 

Inner join Production.Product P ON P.ProductSubcategoryID=PSC.ProductSubcategoryID

Where PC.Name in (@category) and PSC.name in (@subcategory)</CommandText>
      </Query>
      <Fields>
        <Field Name="Product">
          <DataField>Product</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Maindataset">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@category">
            <Value>=Parameters!category.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@subcategory">
            <Value>=Parameters!subcategory.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Product">
            <Value>=Parameters!Product.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT P.name as product,PC.name as Category,PSC.name as Subcategory,SOD.LineTotal,SOD.OrderQty from sales.SalesPerson SP Inner join Sales.SalesOrderHeader SOH
on SP.BusinessEntityID=SOH.SalesPersonID Inner join Sales.SalesOrderDetail SOD
on SOH.SalesOrderID=SOD.SalesOrderID Inner JOin Production.Product P
on SOD.ProductID=P.ProductID Inner join Production.ProductSubcategory PSC
on p.ProductSubcategoryID=PSC.ProductSubcategoryID Inner join Production.ProductCategory PC

On PSC.ProductCategoryID=Pc.ProductCategoryID Where PC.Name in (@category) and PSC.name in (@subcategory) And P.name in (@Product)</CommandText>
      </Query>
      <Fields>
        <Field Name="product">
          <DataField>product</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Category">
          <DataField>Category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Subcategory">
          <DataField>Subcategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LineTotal">
          <DataField>LineTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="OrderQty">
          <DataField>OrderQty</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
      </Fields>
